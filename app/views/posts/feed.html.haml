-feed_title(@tags)

=render :partial=>'tag_cloud', :locals=>{:tags=>@all_tags, :selected=>@selected_tags}

-if @posts.count > 0
  #post_feed
    #post_feed_list
      =render :partial=>'feed_list', :locals=>{:feed_list=>@posts, :tags=>@tags}
  :javascript
    PostFeed.Init();
-else
  .no_entries
    No articles found. Please check later.